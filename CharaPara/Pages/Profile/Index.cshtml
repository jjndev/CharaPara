@page
@model CharaPara.Pages.Profile.IndexModel

@{
    ViewData["Title"] = "Your Profiles";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].ProfileType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].SensitiveStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].Summary)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].VisibleStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].ModerationStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].TextColor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].ProfileColor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].BorderColor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].DateTimeCreated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profile[0].DateTimeModified)
